{
  "name": "nuxt-qrcode",
  "version": "0.4.8",
  "description": "A Nuxt module to provide support for QR Code generation and read.",
  "repository": "https://github.com/sandros94/nuxt-qrcode",
  "homepage": "https://qrcode.s94.dev",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground --tunnel",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare docs",
    "docs": "nuxt dev docs --tunnel",
    "docs:build": "nuxt build docs",
    "docs:preview": "nuxt preview",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm lint && pnpm test:types && vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && nuxt typecheck playground && nuxt typecheck docs"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "@vueuse/core": "^14.1.0",
    "barcode-detector": "^3.0.8",
    "defu": "^6.1.4",
    "uqr": "^0.1.2",
    "vue-qrcode-reader": "^5.7.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.21.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "nuxt": "catalog:",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8"
  },
  "resolutions": {
    "nuxt-qrcode": "workspace:*",
    "brace-expansion": "^2.0.2",
    "dompurify": "^3.2.4",
    "glob": "^10.5.0",
    "js-yaml": "^4.1.1",
    "node-forge": "^1.3.2"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.25.0"
}
