{
  "name": "nuxt-qrcode",
  "version": "0.0.2",
  "description": "A Nuxt module to provide support for QR Code generation and read.",
  "repository": "https://github.com/sandros94/nuxt-qrcode",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "build": {
    "externals": [
      "defu",
      "uqr"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground --tunnel",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "nr lint && nr prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "@vueuse/nuxt": "^11.1.0",
    "barcode-detector": "^2.2.8",
    "uqr": "^0.1.2",
    "vue-qrcode-reader": "^5.5.10"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.4.2",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.1",
    "@nuxt/test-utils": "^3.14.2",
    "changelogen": "^0.5.5",
    "eslint": "^9.10.0",
    "nuxt": "^3.13.2",
    "vite": "^5.4.4",
    "vitepress": "^1.3.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "resolutions": {
    "nuxt-qrcode": "workspace:*"
  }
}
